<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DashboardActivity"
    android:background="@color/primaryColor">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dasboard"
        android:textSize="60dp"
        android:layout_marginTop="50dp"
        android:layout_marginStart="40dp"
        android:fontFamily="sans-serif-black"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_marginTop="130dp"
        android:background="@color/black"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle"
        android:layout_marginTop="180dp"
        android:layout_marginStart="30dp"/>

    <ImageView
        android:id="@+id/btnprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="180dp"
        android:contentDescription="@string/app_name"
        android:onClick="profile"
        android:src="@drawable/profildb" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profil"
        android:textSize="20dp"
        android:layout_marginTop="270dp"
        android:layout_marginStart="70dp"
        android:fontFamily="sans-serif"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle"
        android:layout_marginTop="180dp"
        android:layout_marginStart="210dp" />

    <ImageView
        android:id="@+id/btnpengaduan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="190dp"
        android:layout_marginTop="160dp"
        android:contentDescription="@string/app_name"
        android:onClick="pengaduan"
        android:src="@drawable/pengaduandb" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pengaduan"
        android:textSize="20dp"
        android:layout_marginTop="270dp"
        android:layout_marginStart="230dp"
        android:fontFamily="sans-serif"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle"
        android:layout_marginTop="350dp"
        android:layout_marginStart="30dp"/>

    <ImageView
        android:id="@+id/btnhistoris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="330dp"
        android:contentDescription="@string/app_name"
        android:onClick="historis"
        android:src="@drawable/historisdb" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Historis"
        android:textSize="20dp"
        android:layout_marginTop="440dp"
        android:layout_marginStart="65dp"
        android:fontFamily="sans-serif"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle"
        android:layout_marginTop="350dp"
        android:layout_marginStart="210dp"/>

    <ImageView
        android:id="@+id/btnkeluar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginTop="350dp"
        android:contentDescription="@string/app_name"
        android:onClick="keluar"
        android:src="@drawable/keluardb" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Keluar"
        android:textSize="20dp"
        android:layout_marginTop="443dp"
        android:layout_marginStart="245dp"
        android:fontFamily="sans-serif"/>



</RelativeLayout>